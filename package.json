    "@types/lodash.memoize": "^4.1.8",
    "i18n-js": "^4.3.2",
    "lodash.memoize": "^4.1.2",
    "react": "18.2.0",
    "react-native": "0.72.6",
    "react-native-gesture-handler": "^2.13.4",
